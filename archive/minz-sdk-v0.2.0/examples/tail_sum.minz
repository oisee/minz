// Tail recursive sum example - simpler test
fn sum_tail(n: u16, acc: u16) -> u16 {
    if n == 0 {
        return acc;
    }
    // This is tail recursive - the recursive call is the last operation
    return sum_tail(n - 1, acc + n);
}

fn main() -> void {
    let result = sum_tail(10, 0);  // Should be 55
}