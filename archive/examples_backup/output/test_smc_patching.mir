; MinZ Intermediate Representation (MIR)
; Module: main

Function ...examples.test_smc_patching.add_nums(x: u8, y: u8) -> u8
  Instructions:
      0: 12
      1: 12
      2: r5 = r3 + r4
      3: return r5

Function ...examples.test_smc_patching.calculate() -> u8
  @smc
  Locals:
    r1 = a: u16
    r5 = b: u16
    r9 = c: u16
  Instructions:
      0: r2 = 5
      1: r3 = 3
      2: r4 = call add_nums
      3: store , r4
      4: r6 = 10
      5: r7 = 20
      6: r8 = call add_nums
      7: store , r8
      8: r10 = load a
      9: r11 = load b
     10: r12 = call add_nums
     11: store , r12
     12: r13 = load c
     13: return r13

Function ...examples.test_smc_patching.start() -> u8
  @smc
  Instructions:
      0: r1 = call calculate
      1: return r1

