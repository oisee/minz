; MinZ Intermediate Representation (MIR)
; Module: main

Function examples.test_abi.smc_function(x: u8, y: u8) -> u8
  @smc
  Instructions:
      0: 12
      1: 12
      2: r5 = r3 + r4
      3: return r5

Function examples.test_abi.register_function(a: u8, b: u8) -> u8
  Instructions:
      0: r3 = load a
      1: r4 = load b
      2: r5 = r3 * r4
      3: return r5

Function examples.test_abi.mapped_function(x: u8, y: u8) -> u8
  Instructions:
      0: r3 = load x
      1: r4 = load y
      2: r5 = r3 - r4
      3: return r5

Function examples.test_abi.stack_function(data: u16, count: u8) -> u16
  Instructions:
      0: r3 = load data
      1: r4 = load count
      2: r5 = r3 + r4
      3: return r5

Function examples.test_abi.external_function(ptr: *u8, size: u16) -> void
  Locals:
    r3 = result1: u16
    r7 = result2: u16
    r11 = result3: u16
    r15 = result4: u16
  Instructions:
      0: r4 = 5
      1: r5 = 10
      2: r6 = call smc_function
      3: store , r6
      4: r8 = 3
      5: r9 = 4
      6: r10 = call register_function
      7: store , r10
      8: r12 = 20
      9: r13 = 5
     10: r14 = call mapped_function
     11: store , r14
     12: r16 = 1000
     13: r17 = 50
