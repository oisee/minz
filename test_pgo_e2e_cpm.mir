; MinZ Intermediate Representation (MIR)
; Module: main

Function ...test_pgo_e2e.hot_function$u8(n: u8) -> u8
  @smc
  Instructions:
      0: TRUE_SMC_LOAD ; Load from anchor n$imm0
      1: r3 = 1
      2: r4 = r2 + r3
      3: return r4

Function ...test_pgo_e2e.main() -> void
  @smc
  Locals:
    r1 = result: u8
  Instructions:
      0: PATCH_TEMPLATE
      1: PATCH_TARGET
      2: PATCH_PARAM
      3: PRINT_STRING_DIRECT ; Direct print "Result: " (8 chars)
      4: r0 = call print_u8_decimal ; Call runtime print_u8_decimal
      5: return

