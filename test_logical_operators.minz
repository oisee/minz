// Test logical operators parsing

fun test_simple_or(a: u8, b: u8) -> bool {
    return a > 100 || b > 100;
}

fun test_simple_and(a: u8, b: u8) -> bool {
    return a < 50 && b < 50;
}

fun test_complex(a: u8, b: u8) -> bool {
    if a > 200 || b > 200 {
        return true;
    }
    return false;
}

fun main() -> void {
    let x: u8 = 150;
    let y: u8 = 250;
    
    if test_simple_or(x, y) {
        @print("OR test passed");
    }
    
    if test_simple_and(30, 40) {
        @print("AND test passed");
    }
    
    if test_complex(x, y) {
        @print("Complex test passed");
    }
}