// Test literal patterns in pattern matching

fun number_type(n: u16) -> u16 {
    case n {
        0 => return 1,     // zero
        1 => return 2,     // one
        42 => return 3,    // answer
        _ => return 4,     // other
    }
}

fun main() -> void {
    let result1 = number_type(0);
    let result2 = number_type(1);
    let result3 = number_type(42);
    let result4 = number_type(99);
    
    @print("0 -> ");
    @print(result1);
    @print("\n");
    
    @print("1 -> ");
    @print(result2);
    @print("\n");
    
    @print("42 -> ");
    @print(result3);
    @print("\n");
    
    @print("99 -> ");
    @print(result4);
    @print("\n");
}