// Comprehensive pattern matching test

enum Status {
    Ok,
    Error,
    Pending,
}

enum Color {
    Red,
    Green,
    Blue,
}

fun status_message(s: Status) -> u16 {
    case s {
        Status.Ok => 1,
        Status.Error => 2,
        Status.Pending => 3,
    }
}

fun color_value(c: Color) -> u16 {
    case c {
        Color.Red => 255,
        Color.Green => 65280,
        Color.Blue => 16711680,
    }
}

fun main() -> void {
    let status = Status.Error;
    let msg = status_message(status);
    @print("Status message: ");
    @print(msg);
    @print("\n");
    
    let color = Color.Blue;
    let value = color_value(color);
    @print("Color value: ");
    @print(value);
    @print("\n");
}