; Simple hello world for mze testing
ORG $8000

START:
    LD A, 72        ; Load 'H' (ASCII 72)
    RST $10         ; Print to host stdout via ZX Spectrum ROM
    LD A, 101       ; Load 'e' (ASCII 101)
    RST $10         
    LD A, 108       ; Load 'l' (ASCII 108)
    RST $10         
    LD A, 108       ; Load 'l' (ASCII 108)
    RST $10         
    LD A, 111       ; Load 'o' (ASCII 111)
    RST $10         
    LD A, 33        ; Load '!' (ASCII 33)
    RST $10         
    LD A, 13        ; CR
    RST $10         
    
    ; Proper exit - HALT instruction
    HALT

END