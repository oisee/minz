; MinZ Intermediate Representation (MIR)
; Module: main

Function ...examples.field_assignment.main() -> void
  @smc
  Locals:
    r1 = p: ...examples.field_assignment.Point
    r6 = val: u16
  Instructions:
      0: r2 = 10
      1: r3 = load p
      2: STORE_FIELD ; Store to field x (offset 0)
      3: r4 = 20
      4: r5 = load p
      5: STORE_FIELD ; Store to field y (offset 1)
      6: r7 = load p
      7: LOAD_FIELD ; Load field x (offset 0)
      8: r9 = load p
      9: LOAD_FIELD ; Load field y (offset 1)
     10: r11 = r8 + r10
     11: store , r11
     12: return

