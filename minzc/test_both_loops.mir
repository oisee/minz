; MinZ Intermediate Representation (MIR)
; Module: main

Function ...test_both_loops.main() -> void
  @smc
  Locals:
    r1 = arr1: [2]u8
    r4 = arr2: [2]u8
  Instructions:
      0: r2 = 1
      1: r3 = 2
      2: r5 = 3
      3: r6 = 4
      4: r7 = load arr1
      5: 62 ; Load table base address
      6: 62 ; Load table base for end calculation
      7: 67 ; Calculate table end (+ 2 elements * 1 bytes)
      8: 66 ; Load loop counter
      9: loop_start_1:
     10: 47 ; Check if reached end of table
     11: 3 ; Exit if done
     12: 63 ; Copy element to buffer at $F000
     13: 64 ; Copy buffer back to element at $F000
     14: 67 ; Advance to next element (+1 bytes)
     15: 65 ; Decrement counter and loop if not zero
     16: loop_end_2:
     17: return

