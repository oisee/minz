; MinZ Intermediate Representation (MIR)
; Module: main

Function test_debug_recursion.simple_factorial(n: u8) -> u16
  @smc
  @recursive
  Instructions:
      0: 29 ; Load from anchor n$imm0
      1: r3 = 1
      2: r4 = r2 <= r3
      3: jump_if_not r4, else_1
      4: r5 = 1
      5: return r5
      6: else_1:
      7: 29 ; Load from anchor n$imm0
      8: 29 ; Load from anchor n$imm0
      9: r10 = call simple_factorial
     10: r11 = r6 * r10
     11: return r11

Function test_debug_recursion.main() -> void
  @smc
  Locals:
    r1 = result: u16
  Instructions:
      0: r3 = call simple_factorial
      1: store , r3
      2: return

