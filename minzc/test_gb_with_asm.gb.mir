; MinZ Intermediate Representation (MIR)
; Module: main

Function test_gb.main() -> void
  @smc
  Locals:
    r1 = x: u8
    r3 = y: u8
    r5 = sum: u8
  Instructions:
      0: r2 = 42
      1: store , r2
      2: r4 = 10
      3: store , r4
      4: r6 = load x
      5: r7 = load y
      6: r8 = r6 + r7
      7: store , r8
      8: ASM
      9: PRINT_STRING_DIRECT ; Direct print "Result: " (8 chars)
     10: r9 = load sum
     11: PRINT_U8
     12: PRINT_STRING_DIRECT ; Direct print "
" (1 chars)
     13: return

