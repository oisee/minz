; MinZ Intermediate Representation (MIR)
; Module: main

; Globals:
;   test_array_index.numbers: [5]u8

Function test_array_index.test_array_index() -> u8
  @smc
  Locals:
    r1 = idx: u8
    r3 = value: u8
  Instructions:
      0: r2 = 2
      1: store idx, r2
      2: r4 = addr(test_array_index.numbers)
      3: r5 = load idx
      4: LOAD_INDEX ; Load array element (u8)
      5: store value, r6
      6: r7 = load value
      7: return r7

Function test_array_index.main() -> u8
  @smc
  Locals:
    r1 = result: u8
  Instructions:
      0: r2 = call test_array_index.test_array_index
      1: store result, r2
      2: r3 = load result
      3: return r3

