; MinZ Intermediate Representation (MIR)
; Module: main

Function ......examples.test_smc_patching.add_nums(x: u8, y: u8) -> u8
  @smc
  Instructions:
      0: UNKNOWN_OP_33 ; Load from anchor x$imm0
      1: UNKNOWN_OP_33 ; Load from anchor y$imm0
      2: r5 = r3 + r4
      3: return r5

Function ......examples.test_smc_patching.calculate() -> u8
  @smc
  Locals:
    r1 = a: u16
    r5 = b: u16
    r9 = c: u16
  Instructions:
      0: r4 = call add_nums
      1: r8 = call add_nums
      2: r12 = call add_nums
      3: r13 = load c
      4: return r13

Function ......examples.test_smc_patching.start() -> u8
  @smc
  Instructions:
      0: r1 = call calculate
      1: return r1

