     11: r8 = load x
     12: PRINT_U8
     13: PRINT_STRING_DIRECT ; Direct print ", y = " (6 chars)
     14: r9 = load y
     15: PRINT_U8
     16: PRINT_STRING_DIRECT ; Direct print "Sum: " (5 chars)
     17: PRINT_STRING_DIRECT ; Direct print "Sum: " (5 chars)
     18: r10 = 0
     19: return r10

