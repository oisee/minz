; MinZ Intermediate Representation (MIR)
; Module: main

Function test_peephole.test() -> void
  @smc
  Locals:
    r1 = a: u8
    r3 = b: u8
  Instructions:
      0: r2 = 5
      1: store , r2
      2: r4 = 10
      3: store , r4
      4: r5 = load a
      5: r6 = load b
      6: r7 = r5 < r6
      7: jump_if_not r7, else_1
      8: LOAD_STRING
      9: PRINT_STRING ; Print "a is less than b" (16 chars via loop)
     10: jump end_if_2
     11: else_1:
     12: end_if_2:
     13: return

Function test_peephole.main() -> void
  @smc
  Instructions:
      0: r1 = call test_peephole.test
      1: return

