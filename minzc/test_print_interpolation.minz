// Test @print string interpolation with expressions

fun main() -> u8 {
    // Test constant expressions - these should work
    @print("Simple math: { 2 + 3 }")
    @print("Multiplication: { 5 * 4 }")
    @print("Complex: { (10 + 5) * 2 }")
    
    // Test with variables - these need runtime evaluation
    let x = 10;
    let y = 20;
    @print("x = { x }, y = { y }")
    @print("Sum: { x + y }")
    
    return 0;
}