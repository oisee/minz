fun print_u8(value: u8) -> void {
    let temp: u8 = value;
    
    // Handle hundreds
    if (temp >= 100) {
        print_char(48 + temp / 100);
        temp = temp % 100;
    }
    
    // Handle tens
    if (temp >= 10) {
        print_char(48 + temp / 10);
        temp = temp % 10;
    }
    
    // Always print ones
    print_char(48 + temp);
}

fun print_newline() -> void {
    print_char(13);
    print_char(10);
}

fun main() -> void {
    // Test print_u8
    print_u8(0);
    print_newline();
    print_u8(42);
    print_newline();
    print_u8(123);
    print_newline();
    print_u8(255);
    print_newline();
}