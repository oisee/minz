// Test @define templates - native text substitution!

// Define a template for entity types
@define(entity, health, damage)[[[
    struct {0} {
        health: u8 = {1},
        damage: u8 = {2}
    }
    
    fun spawn_{0}() -> {0} {
        return {0} { health: {1}, damage: {2} };
    }
]]]

// Use the template to generate code
@define("Enemy", 100, 25)
@define("Boss", 500, 50)

fun main() -> u8 {
    let enemy = spawn_Enemy();
    let boss = spawn_Boss();
    
    // Just test that the functions were generated
    return 42;
}