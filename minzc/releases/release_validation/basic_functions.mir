; MinZ Intermediate Representation (MIR)
; Module: main

Function ......examples.basic_functions.add(a: u8, b: u8) -> u8
  @smc
  Instructions:
      0: UNKNOWN_OP_33 ; Load from anchor a$imm0
      1: UNKNOWN_OP_33 ; Load from anchor b$imm0
      2: r5 = r3 + r4
      3: return r5

Function ......examples.basic_functions.main() -> void
  @smc
  Locals:
    r1 = x: u8
    r3 = y: u8
    r5 = result: u8
    r9 = i: u8
  Instructions:
      0: r8 = call add
      1: loop_1:
      2: r11 = load i
      3: r12 = 10
      4: r13 = r11 < r12
      5: jump_if_not r13, end_loop_2
      6: jump loop_1
      7: end_loop_2:
      8: return

