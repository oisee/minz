; MinZ Intermediate Representation (MIR)
; Module: main

Function ......examples.test_true_smc_call.add(x: u8, y: u8) -> u8
  @smc
  Instructions:
      0: UNKNOWN_OP_33 ; Load from anchor x$imm0
      1: UNKNOWN_OP_33 ; Load from anchor y$imm0
      2: r5 = r3 + r4
      3: return r5

Function ......examples.test_true_smc_call.main() -> u8
  @smc
  Locals:
    r1 = result: u16
    r5 = result2: u16
  Instructions:
      0: r4 = call add
      1: r8 = call add
      2: r9 = load result
      3: r10 = load result2
      4: r11 = r9 + r10
      5: return r11

