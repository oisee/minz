; MinZ Intermediate Representation (MIR)
; Module: main

Function ...examples.field_assignment.main() -> void
  @smc
  Locals:
    r1 = p: ...examples.field_assignment.Point
    r2 = val: u16
  Instructions:
      0: r3 = load p
      1: 13 ; Load field x (offset 0)
      2: r5 = load p
      3: 13 ; Load field y (offset 1)
      4: r7 = r4 + r6
      5: store , r7
      6: return

