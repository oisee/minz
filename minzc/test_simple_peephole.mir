; MinZ Intermediate Representation (MIR)
; Module: main

Function examples.test_simple_peephole.test_zero() -> u8
  @smc
  Locals:
    r1 = a: u8
  Instructions:
      0: r3 = load a
      1: return r3

Function examples.test_simple_peephole.test_inc() -> u8
  @smc
  Locals:
    r1 = x: u8
  Instructions:
      0: r3 = load x
      1: r4 = 1
      2: r5 = r3 + r4
      3: return r5

Function examples.test_simple_peephole.main() -> void
  @smc
  Locals:
    r1 = a: u8
    r3 = b: u8
  Instructions:
      0: r2 = r2 ; Inlined return value
      1: r4 = load x ; Inlined from examples.test_simple_peephole.test_inc
      2: r5 = 1 ; Inlined from examples.test_simple_peephole.test_inc
      3: r6 = r4 + r5 ; Inlined from examples.test_simple_peephole.test_inc
      4: r4 = r6 ; Inlined return value
      5: r0 = call print_u8_decimal ; Call runtime print_u8_decimal
      6: r0 = call print_u8_decimal ; Call runtime print_u8_decimal
      7: return

