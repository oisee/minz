; Test for LD (nn), HL (opcode $22)
    ORG $8000
    
    ; Setup if needed
    LD HL, $9000    ; Set HL for indirect operations
    LD BC, $1234    ; Set BC
    LD DE, $5678    ; Set DE
    LD A, $42       ; Set A
    
    ; Test instruction
    LD ($1234), HL
    
    ; Cleanup
    NOP
    RET
    
    END
