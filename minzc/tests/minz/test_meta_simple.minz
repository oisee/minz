// Simple test of metaprogramming features

// Test @minz blocks
@minz[[[
    for i in 0..3 {
        @emit("fun getter_{ i }() -> u8 { return { i }; }")
    }
]]]

// Test @lua blocks
@lua[[[
    for i = 1, 3 do
        print(string.format("const LEVEL_%d: u8 = %d;", i, i * 10))
    end
]]]

fun main() -> void {
    let a = getter_0();
    let b = getter_1();
    @print("Values: { a } and { b }");
    @print("Level 2 = { LEVEL_2 }");
}