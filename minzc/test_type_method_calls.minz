// Test Type.method() call syntax

// Define a Printable interface
interface Printable {
    fun print(self) -> void;
}

// Implement Printable for u8
impl Printable for u8 {
    fun print(self) -> void {
        @print(self);
    }
}

// A struct
struct Point {
    x: u8,
    y: u8,
}

// Implement Printable for Point
impl Printable for Point {
    fun print(self) -> void {
        @print("Point(");
        @print(self.x);
        @print(", ");
        @print(self.y);
        @print(")");
    }
}

fun main() -> void {
    let x: u8 = 42;
    let p = Point{x: 10, y: 20};
    
    // Test Type.method() calls
    u8.print(x);      // Should call the u8 implementation
    Point.print(p);   // Should call the Point implementation
    
    @print("Type method calls work!\n");
}