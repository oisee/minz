; MinZ Intermediate Representation (MIR)
; Module: main

Function ...examples.test_patch.add(x: u8, y: u8) -> u8
  @smc
  Instructions:
      0: 25 ; Load from anchor x$imm0
      1: 25 ; Load from anchor y$imm0
      2: r5 = r3 + r4
      3: return r5

Function ...examples.test_patch.main() -> u8
  @smc
  Locals:
    r1 = a: u8
    r5 = b: u8
  Instructions:
      0: r4 = call add
      1: store , r4
      2: r8 = call add
      3: store , r8
      4: r9 = load a
      5: r10 = load b
      6: r11 = r9 + r10
      7: return r11

