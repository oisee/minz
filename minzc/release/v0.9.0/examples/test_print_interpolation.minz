fun main() -> void {
    let x: u8 = 42;
    let y: u16 = 1000;
    let flag: bool = true;
    
    // Test string interpolation with different types
    @print("The value of x is {x}");
    @print("x = {x}, y = {y}");
    @print("Flag is {flag}");
    
    // Test simple expressions
    @print(x);
    @print(y);
    @print(flag);
    
    // Test string literals without interpolation
    @print("Hello World!");
}