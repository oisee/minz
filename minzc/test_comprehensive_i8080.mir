; MinZ Intermediate Representation (MIR)
; Module: main

; Globals:
;   test_array_comprehensive.numbers: [3]u8
;   test_array_comprehensive.values: [2]u16

Function test_array_comprehensive.test_u8_array() -> u8
  @smc
  Locals:
    r1 = idx: u8
  Instructions:
      0: r2 = 1
      1: store idx, r2
      2: r3 = addr(test_array_comprehensive.numbers)
      3: r4 = load idx
      4: LOAD_INDEX ; Load array element (u8)
      5: return r5

Function test_array_comprehensive.test_u16_array() -> u16
  @smc
  Locals:
    r1 = idx: u8
  Instructions:
      0: r2 = 0
      1: store idx, r2
      2: r3 = addr(test_array_comprehensive.values)
      3: r4 = load idx
      4: LOAD_INDEX ; Load array element (u16)
      5: return r5

Function test_array_comprehensive.main() -> u8
  @smc
  Locals:
    r1 = result1: u8
    r3 = result2: u16
  Instructions:
      0: r2 = call test_array_comprehensive.test_u8_array
      1: store result1, r2
      2: r4 = call test_array_comprehensive.test_u16_array
      3: store result2, r4
      4: r5 = load result1
      5: return r5

