; MinZ Intermediate Representation (MIR)
; Module: main

Function ...test_old_loop.main() -> void
  @smc
  Locals:
    r1 = arr: [2]u8
  Instructions:
      0: r2 = 1
      1: r3 = 2
      2: r4 = load arr
      3: 62 ; Load table base address
      4: 62 ; Load table base for end calculation
      5: 67 ; Calculate table end (+ 2 elements * 1 bytes)
      6: 66 ; Load loop counter
      7: loop_start_1:
      8: 47 ; Check if reached end of table
      9: 3 ; Exit if done
     10: 63 ; Copy element to buffer at $F000
     11: 64 ; Copy buffer back to element at $F000
     12: 67 ; Advance to next element (+1 bytes)
     13: 65 ; Decrement counter and loop if not zero
     14: loop_end_2:
     15: return

