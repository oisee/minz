// Test field access expressions
struct Point {
    x: u8,
    y: u8
}

fun test_field_read() -> u8 {
    let p: Point
    return p.x  // Field access
}

fun test_field_write() -> void {
    let mut p: Point
    p.x = 10  // Field assignment
    p.y = 20
}

fun main() -> void {
    let val = test_field_read()
    test_field_write()
}