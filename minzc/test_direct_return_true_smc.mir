; MinZ Intermediate Representation (MIR)
; Module: main

Function ...examples.test_direct_return.calculate_once(a: u8, b: u8) -> u16
  @smc
  Instructions:
      0: 25 ; Load from anchor a$imm0
      1: return r3

Function ...examples.test_direct_return.calculate_once2(a: u8, b: u8) -> u16
  @smc
  Instructions:
      0: 25 ; Load from anchor a$imm0
      1: return r3

Function ...examples.test_direct_return.main() -> void
  @smc
  Locals:
    r4 = temp: u16
  Instructions:
      0: r3 = call calculate_once ; Optimized: return value directly stored to result
      1: r7 = call calculate_once2
      2: store , r7
      3: r10 = call calculate_once2
      4: store result, r10
      5: return

