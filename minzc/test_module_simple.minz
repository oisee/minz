// Simple test for module function IR generation
import zx.screen as screen;

fun main() -> void {
    // Test calling simple module functions
    let x: u8 = 10;
    let y: u8 = 20;
    
    // Call pixel_addr which doesn't use inline assembly
    let addr = screen.pixel_addr(x, y);
    
    // Call attr_addr 
    let attr = screen.attr_addr(x / 8, y / 8);
    
    @print("Pixel address: ");
    @print(addr);
    @print("\n");
    
    @print("Attr address: ");
    @print(attr);
    @print("\n");
}