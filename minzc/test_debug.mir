; MinZ Intermediate Representation (MIR)
; Module: main

Function test_debug_struct.get_point() -> test_debug_struct.Point
  @smc
  Locals:
    r1 = p: test_debug_struct.Point
  Instructions:
      0: r2 = 5
      1: r3 = load p
      2: STORE_FIELD ; Store to field x (offset 0)
      3: r4 = load p
      4: return r4

Function test_debug_struct.main() -> void
  @smc
  Locals:
    r1 = result: u16
  Instructions:
      0: r2 = call test_debug_struct.get_point
      1: store result, r2
      2: r3 = load result
      3: LOAD_FIELD ; Load field x (offset 0)
      4: r0 = call print_u8_decimal ; Call runtime print_u8_decimal
      5: return

