; MinZ Intermediate Representation (MIR)
; Module: main

Function ...examples.test_16bit_smc.multiply_add(a: u16, b: u16, c: u16) -> u16
  @smc
  Instructions:
      0: LOAD_PARAM
      1: LOAD_PARAM
      2: r6 = r4 * r5
      3: LOAD_PARAM
      4: r8 = r6 + r7
      5: return r8

Function ...examples.test_16bit_smc.main() -> void
  @smc
  Locals:
    r1 = result: u16
  Instructions:
      0: r2 = 10
      1: r3 = 20
      2: r4 = 5
      3: r5 = call multiply_add
      4: store , r5
      5: return

