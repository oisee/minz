// Comprehensive test of module aliasing
import std;                    // No alias - use std.function()
import math as m;              // Short alias
import zx.screen as display;   // Descriptive alias
import zx.input as kbd;        // Keyboard alias

fun main() -> void {
    // Test no alias (std)
    std.cls();
    std.println("=== Module Alias Test ===");
    
    // Test math with short alias
    let x: u8 = 12;
    let y: u8 = 8;
    
    std.print("GCD(12, 8) = ");
    std.println(m.gcd(x, y));  // Using 'm' alias
    
    // Test nested module aliases
    display.set_border(3);      // Using 'display' alias
    display.set_ink(7);
    display.set_paper(0);
    
    // Test multiple aliases in expressions
    if m.is_even(x) {          // math alias
        std.println("12 is even");  // no alias
    }
    
    // Test keyboard module alias
    std.print("Press any key...");
    let key = kbd.wait_key();  // Using 'kbd' alias
    std.print("You pressed: ");
    std.println(key);
    
    // Mix aliases and constants
    let clamped = m.clamp(200, 0, 100);  // Using alias for function
    std.print("200 clamped to 0-100 = ");
    std.println(clamped);
}