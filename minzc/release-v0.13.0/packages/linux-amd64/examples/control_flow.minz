// Control Flow Examples - if, while, loop
fun test_if(x: u8) -> u8 {
    if x > 10 {
        return x * 2;
    } else if x > 5 {
        return x + 10;
    } else {
        return x;
    }
}

fun test_while(n: u8) -> u8 {
    let sum: u8 = 0;
    let i: u8 = 0;
    while i < n {
        sum = sum + i;
        i = i + 1;
    }
    return sum;
}

fun main() {
    let result1 = test_if(15);
    let result2 = test_while(5);
}
