// Test file-based module import
import std;
import math;  // Our new math module!

fun main() -> void {
    std.cls();
    std.println("Testing Math Module!");
    
    // Test square function
    let x: u8 = 5;
    let squared = math.square(x);
    std.print("Square of ");
    std.print(x);
    std.print(" = ");
    std.println(squared);
    
    // Test is_even/is_odd
    let num: u8 = 42;
    if math.is_even(num) {
        std.print(num);
        std.println(" is even");
    }
    
    // Test clamp
    let clamped = math.clamp(150, 0, 100);
    std.print("150 clamped to 0-100 = ");
    std.println(clamped);
    
    // Test GCD
    let a: u8 = 48;
    let b: u8 = 18;
    let g = math.gcd(a, b);
    std.print("GCD of 48 and 18 = ");
    std.println(g);
    
    // Use constants
    std.print("PI * 100 = ");
    std.println(math.PI_TIMES_100);
}