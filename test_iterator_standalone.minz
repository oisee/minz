// Test standalone iterator operations without forEach
// These should compile but not actually execute anything

fun main() -> void {
    let numbers: [u8; 5] = [1, 2, 3, 4, 5];
    
    // These create iterator chains but don't consume them
    // Should compile to no-ops or minimal code
    numbers.map(double);
    numbers.filter(is_even);
    numbers.map(double).filter(is_even);
    
    // Only this one actually does something
    @print("Actually executing:\n");
    numbers.forEach(print_u8);
}

fun double(x: u8) -> u8 { return x * 2; }
fun is_even(x: u8) -> bool { return (x & 1) == 0; }
fun print_u8(x: u8) -> void {}