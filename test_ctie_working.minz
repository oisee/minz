// Simple CTIE test that should definitely work

fun add_numbers(a: u8, b: u8) -> u8 {
    return a + b;
}

fun multiply_by_ten(x: u8) -> u16 {
    return x * 10;
}

fun factorial_five() -> u16 {
    // Hardcoded factorial(5) for simplicity
    return 5 * 4 * 3 * 2 * 1;
}

fun main() -> void {
    // These should all be computed at compile-time
    let result1 = add_numbers(7, 3);     // Should become: LD A, 10
    let result2 = multiply_by_ten(5);    // Should become: LD HL, 50
    let result3 = factorial_five();      // Should become: LD HL, 120
    
    // Print results
    print_u8(result1);
    print_u16(result2);
    print_u16(result3);
}