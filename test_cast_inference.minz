// Test cast type inference issues

fun test_casts() -> void {
    let a: u8 = 10;
    let b: u16 = 1000;
    
    // Cast inference should work
    let c = a as u16;  // Should infer u8 -> u16
    let d = b as u8;   // Should infer u16 -> u8
    
    // Complex cast
    let e = (a + 5) as u16;
    
    // Cast in expressions
    let f: u16 = (a as u16) + b;
    
    @print("c = {}, d = {}, e = {}, f = {}", c, d, e, f);
}

fun main() -> void {
    test_casts();
}