// Test module constants
import zx.screen;

fun main() -> void {
    // Use module constants
    let black = screen.BLACK;
    let red = screen.RED;
    let green = screen.GREEN;
    
    @print("BLACK = ");
    @print(black);
    @print("\n");
    
    @print("RED = ");
    @print(red);
    @print("\n");
    
    @print("GREEN = ");
    @print(green);
    @print("\n");
}