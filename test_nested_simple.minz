// Simple test for nested functions
fun test_nested() -> u8 {
    let x: u8 = 10;
    
    fun helper() -> u8 {
        return x * 2;  // Access parent's x
    }
    
    return helper();
}

fun main() -> void {
    let result = test_nested();
    @print("Result: {}\n", result);  // Should print 20
}