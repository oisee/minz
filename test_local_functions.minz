// Test local function implementation
fun outer(x: u8) -> u8 {
    let factor: u8 = 10;
    
    // Local function that can access parent variables
    fun multiply_by_factor(y: u8) -> u8 {
        return y * factor;  // Should access 'factor' from parent scope
    }
    
    return multiply_by_factor(x);
}

fun main() -> void {
    let result = outer(5);
    @print("Result: {}\n", result);
}