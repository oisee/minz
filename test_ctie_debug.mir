; MinZ Intermediate Representation (MIR)
; Module: main

Function ...test_ctie_debug.add_numbers() -> u8
  @smc
  Instructions:
      0: r1 = 5
      1: r2 = 3
      2: r3 = r1 + r2
      3: return r3

Function ...test_ctie_debug.main() -> void
  @smc
  Locals:
    r1 = result: u16
  Instructions:
      0: PATCH_TEMPLATE
      1: PATCH_TARGET
      2: r2 = 8 ; CTIE: Computed at compile-time (was CALL ...test_ctie_debug.add_numbers)
      3: store result, r2
      4: LOAD_STRING
      5: PRINT_STRING ; Print "Result computed!" (16 chars via loop)
      6: return

