// Test simpler enum usage without pattern matching

enum Color {
    Red,
    Green,
    Blue
}

fun get_value(c: Color) -> u8 {
    // Use if statements instead of pattern matching
    if c == Color.Red {
        return 1;
    }
    if c == Color.Green {
        return 2;
    }
    return 3;
}

fun main() -> void {
    let color = Color.Red;
    let val = get_value(color);
    print_u8(val);
}