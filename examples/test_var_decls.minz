// Test various variable declaration patterns
fun test_var_declarations() -> void {
    // Test 1: Simple let
    let a = 5;
    
    // Test 2: let with type annotation
    let b: u8 = 10;
    
    // Test 3: let mut
    let mut c = 15;
    
    // Test 4: let mut with type annotation  
    let mut d: u16 = 20;
    
    // Test 5: let with expression
    let e = a + b;
    
    // Test 6: let mut with expression (cast to match types)
    let mut f: u16 = (c as u16) + d;
}

fun main() -> void {
    test_var_declarations();
}