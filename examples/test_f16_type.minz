// Test f.16 pure fractional type

fun test_fractional_types() -> void {
    // Test pure fractional types
    let opacity8: f.8 = 0.5;     // 8-bit fraction (50% opacity)
    let opacity16: f.16 = 0.5;    // 16-bit fraction (50% opacity, more precise)
    
    // High precision fractional
    let precise: f.16 = 0.123456; // Much more precision than f.8
    
    // Audio mixing example
    let mix1: f.16 = 0.7;         // 70% of channel 1
    let mix2: f.16 = 0.3;         // 30% of channel 2
    
    // Smooth interpolation
    let t: f.16 = 0.33333;        // Precise 1/3
}