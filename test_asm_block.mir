; MinZ Intermediate Representation (MIR)
; Module: main

Function ...test_asm_block.test_asm() -> u8
  @smc
  Locals:
    r1 = value: u8
  Instructions:
      0: r2 = 42
      1: store value, r2
      2: ASM ; Inline assembly from @asm block
      3: r3 = load value
      4: return r3

Function ...test_asm_block.main() -> void
  @smc
  Locals:
    r1 = result: u8
  Instructions:
      0: PATCH_TEMPLATE
      1: PATCH_TARGET
      2: r2 = call ...test_asm_block.test_asm
      3: store result, r2
      4: r3 = load result
      5: r0 = call print_u8_decimal ; Call runtime print_u8_decimal
      6: return

