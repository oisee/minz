// Test dot notation syntax
struct Point {
    x: u8,
    y: u8,
}

interface Printable {
    fn print(self) -> void;
}

impl Printable for Point {
    fn print(self) -> void {
        print("Point(");  
        print_u8(self.x);
        print(",");
        print_u8(self.y);
        print(")");
    }
}

fn main() -> void {
    let p = Point{x: 10, y: 20};
    
    // Try dot notation (sugar syntax)
    p.print();  // Should be equivalent to Point.print(p)
}